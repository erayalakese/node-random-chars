# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*.js]
charset = utf-8
max_line_length = 80
indent_style = space
indent_size = 2
curly_bracket_next_line = false
quote_type = single
trim_trailing_whitespace = false
insert_final_newline = false
end_of_line = lf
